<%= link_to("<< Back to List", {:action => 'index'}, :class => 'back-link') %>

<div class="accounts show">
  <h2>Show Account</h2>

  <table summary="Account detail view">
    <tr>
      <th>Username</th>
      <td><%= @account.username %></td>
    </tr>
    <tr>
      <th>Email</th>
      <td><%= @account.email %></td>
    </tr>
    <tr>
      <th>Created</th>
      <td><%= @account.created_at %></td>
    </tr>
    <tr>
      <th>Updated</th>
      <td><%= @account.updated_at %></td>
    </tr>
  </table>

</div>
